{
  "name": "@drmente/memed-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Memed API integration",
  "type": "module",
  "main": "./dist/memed-sdk.cjs.js",
  "module": "./dist/memed-sdk.es.js",
  "types": "./dist/memed-sdk.d.ts",
  "exports": {
    ".": {
      "import": "./dist/memed-sdk.es.js",
      "require": "./dist/memed-sdk.cjs.js",
      "types": "./dist/memed-sdk.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "preview": "vite preview"
  },
  "keywords": [
    "memed",
    "sdk",
    "typescript",
    "api",
    "healthcare"
  ],
  "author": "DrMente",
  "license": "MIT",
  "dependencies": {
    "@vercel/node": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.6"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=16.0.0"
  }
}
